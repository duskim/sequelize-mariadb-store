{
  "author": "du kim",
  "name": "@duskim/express-store",
  "version": "0.1.9",
  "license": "MIT",
  "description": "MariaDB session store for express-session",
  "main": "src/main.js",
  "type": "module",
  "homepage": "https://github.com/duskim/sequelize-mariadb-store#readme",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "vitest --no-threads --config ./vitest.config.js --reporter verbose",
    "build": "npx tsc",
    "check": "prettier --check \"src/**/*.ts\"",
    "format": "prettier --write  \"src/**/*.ts\"",
    "lint": "eslint --ext .ts ./src"
  },
  "keywords": [
    "MySQL",
    "MariaDB",
    "Sequelize",
    "express-session",
    "express-session store",
    "es6 modules",
    "typescript"
  ],
  "devDependencies": {
    "@types/express-session": "^1.17.6",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "jsdom": "^21.1.1",
    "prettier": "^2.8.4",
    "typescript": "^5.0.2",
    "vitest": "^0.29.3"
  },
  "dependencies": {
    "@sequelize/core": "^7.0.0-alpha.23",
    "express-session": "^1.17.3",
    "mariadb": "^3.1.0"
  }
}
